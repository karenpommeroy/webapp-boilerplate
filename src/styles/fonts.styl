@font-face {
  font-family: "Baloo"
  font-style: normal
  font-weight: 400
  src: local("Baloo Regular"), local("Baloo-Regular"), url(/assets/fonts/Baloo-Regular.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: normal
    font-weight: 400
    src: local("Lato Regular"), local("Lato-Regular"), url(/assets/fonts/Lato-Regular.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: normal
    font-weight: 700
    src: local("Lato Bold"), local("Lato-Bold"), url(/assets/fonts/Lato-Bold.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: normal
    font-weight: 900
    src: local("Lato Black"), local("Lato-Black"), url(/assets/fonts/Lato-Black.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: normal;
    font-weight: 200
    src: local("Lato Light"), local("Lato-Light"), url(/assets/fonts/Lato-Light.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: normal;
    font-weight: 100
    src: local("Lato Thin"), local("Lato-Thin"), url(/assets/fonts/Lato-Thin.ttf) format("ttf")
}

@font-face {
    font-family: "Lato"
    font-style: italic;
    font-weight: 400
    src: local("Lato Italic"), local("Lato-Italic"), url(/assets/fonts/Lato-Italic.ttf) format("ttf")
}
