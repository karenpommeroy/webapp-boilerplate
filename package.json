{
  "name": "boilerplate-webapp",
  "version": "1.0.0",
  "description": "Web Application Boilerplate",
  "author": "Marcin Karpi≈Ñski <mkarpins@gmail.com>",
  "repository": {
    "url": "https://github.com/karenpommeroy/webapp-boilerplate"
  },
  "main": "./dist/index.js",
  "license": "MIT",
  "packageManager": "yarn@4.9.4",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "compile && yarn tscheck",
    "lint": "eslint",
    "tscheck": "yarn tsc --project ./tsconfig.json",
    "build": "yarn clean && ts-node-dev --transpile-only ./webpack/webpack.config.ts && cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.ts --progress",
    "build:dev": "yarn clean && ts-node-dev --transpile-only ./webpack/webpack.config.ts && cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.ts --progress",
    "start": "yarn start:dev",
    "start:dev": "ts-node-dev --transpile-only ./webpack/webpack.config.ts && cross-env NODE_ENV=development webpack-dev-server --hot --config ./webpack/webpack.config.ts",
    "start:prod": "cross-env NODE_ENV=production webpack serve --config ./webpack/webpack.config.ts",
    "package": "yarn clean && ts-node-dev --transpile-only ./webpack/webpack.config.ts && webpack --config ./webpack/webpack.config.ts --progress",
    "playwright:test": "playwright test",
    "playwright:ui": "playwright test --ui",
    "playwright:debug": "playwright test --debug",
    "playwright:report": "playwright --show-report",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@toolpad/core": "^0.16.0",
    "classnames": "^2.5.1",
    "fs-extra": "^11.3.1",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "i18next-xhr-backend": "^3.2.2",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
