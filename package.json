    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^8.42.0",
    "circular-dependency-plugin": "^5.2.2",
